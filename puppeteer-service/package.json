{
  "name": "puppeteer-service",
  "version": "1.0.0",
  "description": "Puppeteer service using Browserless Chrome instance",
  "main": "dist/server.js",
  "dependencies": {
    "puppeteer": "latest",
    "lodash": "^4.17.11",
    "express": "^4.16.4",
    "express-session": "^1.16.1",
    "express-validator": "^5.3.1",
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "dotenv": "^7.0.0",
    "request": "^2.88.0",
    "lusca": "^1.6.1",
    "request-promise": "^4.2.4",
    "reflect-metadata": "^0.1.13",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/puppeteer": "latest",
    "@types/async": "^2.0.45",
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/bluebird": "^3.5.20",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^0.0.35",
    "@types/cookiejar": "^2.1.0",
    "@types/dotenv": "^6.1.1",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.16.1",
    "@types/express-session": "^1.15.12",
    "@types/jest": "^22.1.3",
    "@types/sinon": "5.0.5",
    "@types/joi": "^13.0.8",
    "@types/lodash": "^4.14.123",
    "@types/lusca": "^1.5.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^11.13.4",
    "@types/nodemailer": "^4.3.4",
    "@types/request": "^2.48.1",
    "@types/request-promise": "^4.1.42",
    "@types/supertest": "^2.0.4",
    "@types/uuid": "^3.4.3",
    "chai": "^4.1.2",
    "concurrently": "^4.1.0",
    "jest": "^22.0.4",
    "mock-express-request": "^0.2.2",
    "mock-express-response": "^0.2.2",
    "node-sass": "^4.7.2",
    "nodemon": "^1.18.11",
    "shelljs": "^0.8.1",
    "supertest": "^3.0.0",
    "ts-jest": "^22.0.4",
    "ts-node": "^8.0.3",
    "tslint": "^5.15.0",
    "typescript": "^3.4.3"
  },
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch-tslint": "nodemon --exec 'npm run tslint || exit 1'",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-tslint\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect=0.0.0.0:9229 dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-tslint\" \"npm run serve-debug\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yatsyukv/puppeteer-service.git"
  },
  "author": "Seva Yatsiuk",
  "bugs": {
    "url": "https://github.com/yatsyukv/puppeteer-service/issues"
  },
  "homepage": "https://github.com/yatsyukv/puppeteer-service#readme"
}
